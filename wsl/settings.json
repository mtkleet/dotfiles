{
  "$help": "https://aka.ms/terminal-documentation",
  "$schema": "https://aka.ms/terminal-profiles-schema",

  // Launch settings
  "initialCols": 165,
  "initialRows": 40,
  "launchMode": "focus",
  "alwaysOnTop": false,
  "language": "en-US",
  "theme": "dark",

  // Selection
  "copyOnSelect": true,
  "copyFormatting": "none",
  "largePasteWarning": false,
  "multiLinePasteWarning": false,
  "wordDelimiters": " /\\()\"'-.,:;<>~!@#$%^&*|+=[]{}~?\u2502",

  // Tab UI
  "alwaysShowTabs": false,
  "showTabsInTitlebar": true,
  "tabSwitcherMode": "disabled",
  "showTerminalTitleInTitlebar": true,
  "tabWidthMode": "equal",
  "useAcrylicInTabRow": true,

  // Miscellaneous
  "autoHideWindow": true,
  "alwaysShowNotificationIcon": true,
  "minimizeToNotificationArea": false,
  "windowingBehavior": "useExisting",
  "confirmCloseAllTabs": true,
  "disableAnimations": true,
  "experimental.rendering.forceFullRepaint": false,

  // Profile management
  "confirmCloseAllTabs": true,
  "defaultProfile": "{574e775e-4f2a-5b96-ac1e-a2962a402336}",
  "hidden": false,
  "disabledProfileSources": ["Windows.Terminal.Azure"],
  "profiles": { 
    "defaults": {
      "adjustIndistinguishableColors": "indexed",
      "antialiasingMode": "cleartype",
      "bellStyle": "taskbar",
      "colorScheme": "One Half Dark",
      "cursorShape": "emptyBox",
      "font": { "face": "MesloLGS NF", "size": 9.0 },
      "historySize": 50000,
      "intenseTextStyle": "bright",
      "opacity": 85,
      "padding": "0",
      "scrollbarState": "hidden",
      "snapOnInput": false,
      "startingDirectory": "%USERPROFILE%",
      "useAcrylic": true,
			"altGrAliasing": true,
      "icon": "%USERPROFILE%\\.config\\ProfileIcons\\terminal.ico"
  },
  "list": 
  [
    {
      "guid": "{574e775e-4f2a-5b96-ac1e-a2962a402336}",
      "icon": "%USERPROFILE%\\.config\\ProfileIcons\\pwsh_core.png",
      "name": "PowerShell",
      "source": "Windows.Terminal.PowershellCore"
    },
    {
      "commandline": "%SystemRoot%\\System32\\cmd.exe /K %USERPROFILE%\\.config\\init.cmd",
      "elevate": true,
      "guid": "{0caa0dad-35be-5f56-a8ff-afceeeaa6101}",
      "icon": "%USERPROFILE%\\.config\\ProfileIcons\\cmd.png",
      "name": "Command Prompt"
    },
    {
      "adjustIndistinguishableColors": "always",
      "colorScheme": "Solarized Dark - Patched",
      "guid": "{a5a97cb8-8961-5535-816d-772efe0c6a3f}",
      "icon": "%USERPROFILE%\\.config\\ProfileIcons\\arch.png",
      "intenseTextStyle": "bold",
      "name": "Arch",
      "opacity": 85,
      "source": "Windows.Terminal.Wsl",
      "startingDirectory": "~"
    },
    {
      "elevate": true,
      "guid": "{61c54bbd-c2c6-5271-96e7-009a87ff44bf}",
      "icon": "%USERPROFILE%\\.config\\ProfileIcons\\pwsh.png",
      "name": "Windows PowerShell"
    } ] 
  },

